        android:icon="@drawable/ic_launcher_contacts"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.prizm.myschool.MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.prizm.myschool.FoodActivity"
            android:label="@string/title_activity_food"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.prizm.myschool.ScheduleActivity"
            android:label="학사일정"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.prizm.myschool.EasterEgg"
            android:label="@string/title_activity_easter_egg"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.prizm.myschool.FindSchool"
            android:label="@string/title_activity_find_school"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.prizm.myschool.AreaActivity"
            android:label="@string/title_activity_area"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.prizm.myschool.LoadFood"
            android:label="@string/title_activity_load_food"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.prizm.myschool.Setting"
            android:label="환경설정"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.prizm.myschool.Developer"
            android:label="@string/title_activity_developer" >
        </activity>
    </application>

</manifest>
